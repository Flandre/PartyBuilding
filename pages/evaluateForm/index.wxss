.user-info {
	padding: 30rpx;
	display: flex;
	justify-content: flex-start;
	align-items: center;
	border-bottom: 1rpx solid #dbdbdb;
	margin-bottom: 30rpx;
}
.user-info .avatar{
	width: 72rpx;
	height: 72rpx;
	border-radius: 36rpx;
	margin-right: 30rpx;
}
.user-info .name{
	font-size: 28rpx;
}

.submit-success {
	display: block;
	padding: 40rpx 0 60rpx;
	text-align: center;
	font-size: 28rpx;
	line-height: 40rpx;
	color: #E60012;
}


.question-list {
	padding: 0 30rpx;
	color: #333;
}
.question-list .question-item{
	margin-bottom: 30rpx;
	font-size: 28rpx;
	line-height: 44rpx;
}
.question-list .question-item .title{
	margin-bottom: 20rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.question-list .question-item .title .main{
	flex-grow: 1;
}
.question-list .question-item .title .judge{
	width: 40rpx;
	height: 40rpx;
	display: block;
	flex-shrink: 0;
}
.question-list .question-item .judge-container{
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.question-list .question-item .judge-container .judge-label{
	flex-grow: 1;
}
.question-list .question-item .judge-container .answer{
	width: 28rpx;
	height: 28rpx;
	display: block;
	flex-shrink: 0;
}


.question-list .question-item .title .highlight{
	color: #E60012;
}
.question-list .question-item.ridio-type .radio{
	display: block;
	line-height: 88rpx;
	white-space: nowrap;
}
.question-list .question-item.ridio-type .radio radio .wx-radio-input{
	border:none;
	width: 52rpx;
	height: 88rpx;
	border-radius: 0;
	padding: 0;
	margin: 0;
	background: url("data:image/png;base64,");
	background-size: 32rpx 32rpx;
	background-repeat: no-repeat;
	background-position: 0 28rpx;
}
.question-list .question-item.ridio-type .radio radio .wx-radio-input.wx-radio-input-checked{
	border:none;
	width: 52rpx;
	height: 88rpx;
	border-radius: 0;
	padding: 0;
	margin: 0;
	background: url("data:image/png;base64,");
	background-size: 32rpx 32rpx;
	background-repeat: no-repeat;
	background-position: 0 28rpx;
}
.question-list .question-item.ridio-type .radio radio .wx-radio-input.wx-radio-input-checked::before{
	content: ''
}

.question-list .question-item.checkbox-type .checkbox{
	display: block;
	line-height: 88rpx;
	white-space: nowrap;
}
.question-list .question-item.checkbox-type .checkbox checkbox .wx-checkbox-input{
	border:none;
	width: 52rpx;
	height: 88rpx;
	border-radius: 0;
	padding: 0;
	margin: 0;
	background: url("data:image/png;base64,");
	background-size: 32rpx 32rpx;
	background-repeat: no-repeat;
	background-position: 0 28rpx;
}
.question-list .question-item.checkbox-type .checkbox checkbox .wx-checkbox-input.wx-checkbox-input-checked{
	border:none;
	width: 52rpx;
	height: 88rpx;
	border-radius: 0;
	padding: 0;
	margin: 0;
	background: url("data:image/png;base64,");
	background-size: 32rpx 32rpx;
	background-repeat: no-repeat;
	background-position: 0 28rpx;
}
.question-list .question-item.checkbox-type .checkbox checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
	content: ''
}

.question-list .question-item.textarea-type textarea{
	display: block;
	width: 630rpx;
	margin-top: 20rpx;
	padding: 18rpx 30rpx;
	border: 1rpx solid #dbdbdb;
}
.question-list .submit-btn{
	background: #E60012;
	color: #fff;
	margin-top: 80rpx;
	margin-bottom: 40rpx;
}
